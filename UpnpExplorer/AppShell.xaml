<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="UpnpExplorer.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:UpnpExplorer"
    Title="UpnpExplorer"
    Shell.BackgroundColor="DarkGreen"
    xmlns:strings="clr-namespace:UpnpExplorer.Resources.Strings">
    <FlyoutItem Title="{x:Static strings:AppResources.DevicesPageTitle}">
        <ShellContent ContentTemplate="{DataTemplate local:Views.DevicesPage}"/>
    </FlyoutItem>
    <FlyoutItem Title="{x:Static strings:AppResources.AboutPageTitle}">
        <ShellContent ContentTemplate="{DataTemplate local:Views.AboutPage}"/>
    </FlyoutItem>
</Shell>
